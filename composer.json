{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^8.1",
    "calebporzio/sushi": "^2.1",
    "cviebrock/eloquent-sluggable": "*",
    "doctrine/dbal": "^3.5",
    "google/recaptcha": "~1.1",
    "guzzlehttp/guzzle": "^7.2",
    "inertiajs/inertia-laravel": "*",
    "kblais/laravel-uuid": "*",
    "laravel-notification-channels/fcm": "dev-development",
    "laravel/framework": "^10.0",
    "laravel/helpers": "^1.1",
    "laravel/horizon": "^5",
    "laravel/sanctum": "^3.2",
    "laravel/slack-notification-channel": "^2.3",
    "laravel/socialite": "^5",
    "laravel/tinker": "^2.8",
    "laravel/ui": "^4.2",
    "predis/predis": "~1.0",
    "reinink/remember-query-strings": "^0.1.0",
    "spatie/browsershot": "^3.4",
    "spatie/eloquent-sortable": "^4.0.1",
    "spatie/laravel-honeypot": "^4.0",
    "symfony/http-client": "^6.0",
    "tedivm/jshrink": "^1.3",
    "tightenco/ziggy": "^1.2",
    "tucker-eric/eloquentfilter": "^3.1"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.2",
    "barryvdh/laravel-ide-helper": "^2.8",
    "fakerphp/faker": "^1.9.1",
    "mockery/mockery": "^1.4.4",
    "nunomaduro/larastan": "^2.0",
    "pestphp/pest": "*",
    "pestphp/pest-plugin-laravel": "*",
    "phpunit/phpunit": "^10.0",
    "spatie/laravel-ignition": "^2.0"
  },
  "autoload": {
    "files": [
      "app/Utilities/helpers.php"
    ],
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "fix-style": "vendor\\bin\\pint",
    "analyse": "vendor\\bin\\phpstan analyse",
    "helper": "php artisan ide-helper:generate && php artisan ide-helper:meta && php artisan ide-helper:models -W"
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "path",
      "url": "./packages/fcm"
    },
    {
      "url": "https://github.com/TahsinGokalp/laravel-uuid.git",
      "type": "git"
    }
  ]
}
