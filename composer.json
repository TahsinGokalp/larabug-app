{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^8.1",
    "calebporzio/sushi": "^2.1",
    "cviebrock/eloquent-sluggable": "^9.0.0",
    "doctrine/dbal": "^3.5",
    "filament/filament": "^2.5",
    "google/recaptcha": "~1.1",
    "guzzlehttp/guzzle": "^7.3",
    "inertiajs/inertia-laravel": "^v0.5.4",
    "kblais/laravel-uuid": "^2.0.0",
    "larabug/larabug": "^2.4",
    "laravel-notification-channels/fcm": "*",
    "laravel/framework": "^9.0.2",
    "laravel/helpers": "^1.1",
    "laravel/horizon": "^5",
    "laravel/octane": "^1.0",
    "laravel/slack-notification-channel": "^2.3",
    "laravel/socialite": "^5",
    "laravel/tinker": "^2.0",
    "laravel/ui": "^3.2",
    "predis/predis": "~1.0",
    "reinink/remember-query-strings": "^0.1.0",
    "spatie/browsershot": "^3.4",
    "spatie/eloquent-sortable": "^4.0.1",
    "spatie/laravel-cookie-consent": "^3.2",
    "spatie/laravel-honeypot": "^4.0",
    "spatie/laravel-ray": "^1.26",
    "spatie/laravel-sitemap": "^6.0.4",
    "spiral/roadrunner": "^2.0",
    "symfony/http-client": "^6.0",
    "symfony/postmark-mailer": "^6.0",
    "tedivm/jshrink": "^1.3",
    "tightenco/ziggy": "^1.2",
    "tucker-eric/eloquentfilter": "^3.1"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.2",
    "barryvdh/laravel-ide-helper": "^2.8",
    "spatie/laravel-ignition": "^1.0",
    "fakerphp/faker": "^1.9.1",
    "friendsofphp/php-cs-fixer": "^3.1.0",
    "mockery/mockery": "^1.4.2",
    "nunomaduro/collision": "^6.1",
    "pestphp/pest": "^1.20",
    "pestphp/pest-plugin-laravel": "^1.1"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "files": [
      "app/Utilities/helpers.php"
    ],
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "classmap": [
    ],
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "post-update-cmd": [
      "@php artisan horizon:publish"
    ],
    "format": [
      "vendor/bin/php-cs-fixer fix"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "barryvdh/laravel-debugbar",
        "barryvdh/laravel-ide-helper"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "path",
      "url": "./packages/fcm"
    }
  ]
}
